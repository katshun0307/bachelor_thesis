\newcommand{\EAppAbs}{
    \infax[\fauxsc{E-AppAbs}]{
        \myval{(\mylam{x}{T}{t})\mysp v}{d} \barrow \myval{[x \mapsto v] t}{d}
        \andalso (d \le 0)
    }
}

\newcommand{\EAbs}{
    \infrule[E-Abs]{
        \myval{t}{d} \rightarrow \myval{t'}{d}
    }{
        \myval{\mylam{x}{T}{t}}{d} \rightarrow \myval{\mylam{x}{T}{t'}}{d} 
    }
}

\newcommand{\EAppOne}{
    \infrule[E-App1]{
        \myval{t_1}{d} \rightarrow \myval{t_1'}{d}
    }{
        \myval{t_1 \mysp t_2}{d} \rightarrow \myval{t_1' \mysp t_2}{d}
    }
}

\newcommand{\EAppTwo}{
    \infrule[E-App2]{
        \myval{t_2}{d'} \rightarrow \myval{t_2'}{d}
    }{
        \myval{v_1 \mysp t_2}{d} \rightarrow \myval{v_1 \mysp t_2'}{d}
    }
}

\newcommand{\EStageBeta}{
    % \infrule[E-StageBeta]{
    %     \myval{t}{d} \rightarrow \myval{t'}{d}
    %     \andalso d \le 1
    %     }{
    %         \myval{\myprev{\mynext{t}}}{d} \rightarrow \myval{t'}{d}
    % }
    \infax[\fauxsc{E-StageBeta}]{
        \myval{\myprev{\mynext{t}}}{d} \starrow \myval{t'}{d}
        \andalso (d \le 0)
    }
}

\newcommand{\EStageEta}{
    % \infrule[E-StageEta]{
    %     \myval{t}{d} \rightarrow \myval{t'}{d} 
    %     \andalso d \le 1
    % }{
    %     \myval{\mynext{\myprev{t}}}{d} \rightarrow \myval{t'}{d}
    % }
    \infax[\fauxsc{E-StageEta}]{
        \myval{\mynext{\myprev{t}}}{d} \starrow \myval{t'}{d}
        \andalso (d \le 1)
    }
}

\newcommand{\ENext}{
    \infrule[E-Next]{
        \myval{t}{d+1} \rightarrow \myval{t'}{d+1}
        \andalso d \le 1    
    }{
        \myval{\mynext{t}}{d} \rightarrow \myval{\mynext{t'}}{d}
    }
}

\newcommand{\EPrev}{
    \infrule[E-Prev]{
        \myval{t}{d-1} \rightarrow \myval{t'}{d-1}
        \andalso d \le 1    
    }{
        \myval{\myprev{t}}{d} \rightarrow \myval{\myprev{t'}}{d}
    }
}

\newcommand{\EEq}{
    \infax[\fauxsc{E-Equal}]{
        \myval{\myidpeel{\myid{a}}{x}{t}}{d} \eqarrow \myval{[x \mapsto a] t}{d}
        \andalso (d = 0)
    }
}

