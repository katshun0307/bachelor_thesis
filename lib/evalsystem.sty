\newcommand{\ETStaged}{
    \infax[\fauxsc{ET-Staged}]{
        t \rightarrow \myval{t}{d}
    }
}

\newcommand{\ETUnstage}{
    \infax[\fauxsc{EV-Unstage}]{
        \myval{v}{d} \rightarrow v
    }
}

