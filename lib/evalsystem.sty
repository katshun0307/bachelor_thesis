\newcommand{\EAppAbs}{
    \infax[\fauxsc{E-Beta}]{
        \myval{(\mylam{x}{T}{s})\mysp t}{d} \barrow_{i} \myval{[x \mapsto t] s}{d}
        \andalso (i = 0)
    }
}

\newcommand{\EAbs}{
    \infrule[E-Abs]{
        \myval{t}{d} \iarrow{i} \myval{t'}{d}
    }{
        \myval{\mylam{x}{T}{t}}{d} \iarrow{i} \myval{\mylam{x}{T}{t'}}{d} 
    }
}

\newcommand{\EAppOne}{
    \infrule[E-App1]{
        \myval{t_1}{d} \iarrow{i} \myval{t_1'}{d}
    }{
        \myval{t_1 \mysp t_2}{d} \iarrow{i} \myval{t_1' \mysp t_2}{d}
    }
}

\newcommand{\EAppTwo}{
    \infrule[E-App2]{
        \myval{t_2}{d'} \iarrow{i} \myval{t_2'}{d}
    }{
        \myval{v_1 \mysp t_2}{d} \iarrow{i} \myval{v_1 \mysp t_2'}{d}
    }
}

\newcommand{\EStageBeta}{
    % \infrule[E-StageBeta]{
    %     \myval{t}{d} \iarrow{i} \myval{t'}{d}
    %     \andalso d \le 1
    %     }{
    %         \myval{\myprev{\mynext{t}}}{d} \iarrow{i} \myval{t'}{d}
    % }
    \infax[\fauxsc{E-StageBeta}]{
        \myval{\myprev{\mynext{t}}}{d} \stbarrow_{i} \myval{t}{d}
        \andalso (i \le 1)
    }
}

\newcommand{\EStageEta}{
    % \infrule[E-StageEta]{
    %     \myval{t}{d} \iarrow{i} \myval{t'}{d} 
    %     \andalso d \le 1
    % }{
    %     \myval{\mynext{\myprev{t}}}{d} \iarrow{i} \myval{t'}{d}
    % }
    \infax[\fauxsc{E-StageEta}]{
        \myval{\mynext{\myprev{t}}}{d} \stearrow_{i} \myval{t}{d}
        \andalso (i = 0)
    }
}

\newcommand{\ENext}{
    \infrule[E-Next]{
        \myval{t}{d+1} \iarrow{i+1} \myval{t'}{d+1}
        \andalso i \le 1    
    }{
        \myval{\mynext{t}}{d} \iarrow{i} \myval{\mynext{t'}}{d}
    }
}

\newcommand{\EPrev}{
    \infrule[E-Prev]{
        \myval{t}{d-1} \iarrow{i-1} \myval{t'}{d-1}
        \andalso i \le 1
    }{
        \myval{\myprev{t}}{d} \iarrow{i} \myval{\myprev{t'}}{d}
    }
}

\newcommand{\EEq}{
    \infax[\fauxsc{E-Equal}]{
        \myval{\myidpeel{\myid{a}}{x}{t}}{d} \eqarrow_{i} \myval{[x \mapsto a] t}{d}
        \andalso (i = 0)
    }
}

